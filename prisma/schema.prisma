
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  username       String   @unique
  name    String
  fatherName     String
  motherName     String
  dob            String
  course         String?
  certificateId  String?   @unique
  totalMarks     String?
  grade          String?
}